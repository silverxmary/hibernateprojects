<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="educacionit.clase8.alarmmanager" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AlarmManagerActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RTCAlarmActivity"
            android:label="@string/title_activity_rtcalarm" >
        </activity>
        <activity
            android:name=".AlarmActivity"
            android:label="@string/title_activity_alarm" >
        </activity>
        <activity
            android:name=".TiempoTranscurridoAlarmActivity"
            android:label="@string/title_activity_tiempo_transcurrido_alarm" >
        </activity>
    </application>

</manifest>
